{
  "name": "WonderPush",
  "module_name": "WonderPush",
  "documentation_url": "https://wonderpush.github.io/wonderpush-ios-sdk/",
  "version": "4.0.4",
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE.txt"
  },
  "platforms": {
    "ios": "9.0"
  },
  "summary": "Push notification library by WonderPush",
  "description": "Push notifications iOS library by WonderPush - cross platform push notifications service. https://www.wonderpush.com",
  "homepage": "https://www.wonderpush.com/",
  "authors": {
    "WonderPush": "contact@wonderpush.com"
  },
  "source": {
    "git": "https://github.com/wonderpush/wonderpush-ios-sdk.git",
    "tag": "v4.0.4"
  },
  "requires_arc": true,
  "default_subspecs": ["Core"],
  "subspecs": [
    {
      "name": "Core",
      "frameworks": [
        "SystemConfiguration",
        "CoreGraphics",
        "UIKit",
        "CoreTelephony",
        "CoreLocation",
        "WebKit"
      ],
      "dependencies": {
      },
      "public_header_files": [
        "WonderPush/Public/WonderPush/*.h"
      ],
      "source_files": [
        "WonderPush/Sources/*.{m,h}",
        "WonderPush/Segmentation/*.{m,h}",
        "WonderPush/InAppMessaging/*.{m,h}"
      ],
      "resource_bundles": {
        "WonderPush": [
          "WonderPush/Resources/*.{lproj,storyboard,png,json}"
        ]
      }
    }
  ]
}
